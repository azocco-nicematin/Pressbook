                                                            <th class="middle"
                                                            style="font-weight: bold; text-align: center; vertical-align: middle">
                                                            Lieu dépot</th>
                                                            <th class="middle"
                                                            style="font-weight: bold; text-align: center; vertical-align: middle">
                                                            Manager</th>
                                                           


                                                    </tr>
